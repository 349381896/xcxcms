.goods_list{overflow:hidden;}.goods_list .goods_info{background-color:#fff;padding:5px 10px 5px 10px;}
.goods_list .li{margin-bottom:4px;width:50%;float:left;overflow:hidden;}
.goods_list .li navigator{display:block;margin-right:2px;background-color:#fff;}.goods_list .goods_tit{overflow:hidden;white-space:nowrap;color:#000}
.goods_list .li image{width:100%;height:48vw;}.goods_list .li .goods_price{color:#f23030;}.goods_list .li .goods_price b{font-size:1rem;font-weight:normal;}.goods_list .li .goods_price span{color:#999;font-size:14px;}
.goods_list .li:nth-child(even) navigator{margin-right:0;margin-left:2px;}
